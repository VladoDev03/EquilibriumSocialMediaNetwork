<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Equilibrium</title>
    <link rel="SHORTCUT ICON" href="~/Images/From app/logo.ico" />
    <link rel="stylesheet" href="~/css/login.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Dancing+Script">

</head>
<body>
    <div class="full-page-background"></div>
    <div class="full-page">
        <div class="navbar">
            <div> <a style="color: #3aafa9; font-family: 'Dancing Script'; font-size: 34px;">Equilibrium</a> </div>
        </div>
        <div class='login-page'>
            <div class="form-box">
                @RenderBody()
            </div>
        </div>
    </div>

    <script scr="~/js/login.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
